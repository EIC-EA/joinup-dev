<?php

/**
 * @file
 * Provides functionality for the Joinup privacy module.
 */

declare(strict_types = 1);

/**
 * Implements hook_page_attachments().
 */
function joinup_privacy_page_attachments(array &$attachments) {
  $attachments['#attached']['library'][] = 'oe_webtools/drupal.webtools-smartloader';
  $attachments['#attached']['library'][] = 'joinup_privacy/joinup_privacy.cck';
}
